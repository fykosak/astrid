#!/bin/bash


USER=user
PASS=password
HOST=astrid.example.org
URL="/build/reponame"


function touch_astrid {
    # timeout 1 second, 1 try
    wget -T 1 -t 1 --http-user=$USER --http-password=$PASS -O /dev/null https://${HOST}$URL 2>/dev/null &
    echo "$URL: rebuild request sent to Astrid."
}


touch_astrid
